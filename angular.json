{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/host",
      "sourceRoot": "projects/host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/host",
            "index": "projects/host/src/index.html",
            "browser": "projects/host/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/host/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/host/public"
              }
            ],
            "server": "projects/host/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/host/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/host/src/app/app.config.ts",
                  "with": "projects/host/src/app/app.config.mf.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.development.ts"
                }
              ]
            },
            "debug": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.development.ts"
                },
                {
                  "replace": "projects/host/src/app/app.config.ts",
                  "with": "projects/host/src/app/app.config.mf.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "host:build:production"
            },
            "development": {
              "buildTarget": "host:build:development"
            },
            "debug": {
              "buildTarget": "host:build:debug"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/host/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/host/public"
              }
            ]
          }
        }
      }
    },
    "section": {
      "projectType": "library",
      "root": "projects/section",
      "sourceRoot": "projects/section",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
