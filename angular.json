{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "host": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/host",
      "sourceRoot": "projects/host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/host/src/main.ts",
            "tsConfig": "projects/host/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/host/public"
              }
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/host/src/app/app-config.ts",
                  "with": "projects/host/src/app/app-config-mf.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.development.ts"
                }
              ]
            },
            "debug": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.development.ts"
                },
                {
                  "replace": "projects/host/src/app/app-config.ts",
                  "with": "projects/host/src/app/app-config-mf.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "host:build:production"
            },
            "development": {
              "buildTarget": "host:build:development"
            },
            "debug": {
              "buildTarget": "host:build:debug"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    },
    "section": {
      "projectType": "library",
      "root": "projects/section",
      "sourceRoot": "projects/section/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/section/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/section/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/section/tsconfig.spec.json"
          }
        }
      }
    },
    "standalone": {
      "projectType": "library",
      "root": "projects/standalone",
      "sourceRoot": "projects/standalone/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/standalone/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/standalone/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/standalone/tsconfig.spec.json"
          }
        }
      }
    },
    "@mf/integration": {
      "projectType": "library",
      "root": "projects/mf/integration",
      "sourceRoot": "projects/mf/integration/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
