{
  "name": "myws",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "export:mf1": "ng build mf1 && cross-env MF_NAME=mf1 gulp",
    "export:mf2": "ng build mf2 && cross-env MF_NAME=mf2 gulp",
    "export:mf3": "ng build mf3 && cross-env MF_NAME=mf3 gulp",
    "export:mf4": "ng build mf4 && cross-env MF_NAME=mf4 gulp",
    "export:all": "npm run export:mf1 && npm run export:mf2 && npm run export:mf3 && npm run export:mf4",
    "start:host": "npm run export:all && npm run start host"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/elements": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "cross-env": "^7.0.3",
    "gulp": "^5.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.1.0",
    "typescript": "~5.5.2"
  }
}
